{% extends "base.twig" %}
{% block content_wrapper %}
<div class="row">
  <div class="col-sm-3">
    <div class="rcptt-sidebar">
      <ul class="nav">
	{% for q in site.faq %}
	<li {% if q.slug == page.slug %}class="active" {% endif %}><a href="{{site.url}}/documentation/{{q.url}}">{{q.name}}</a>
	</li>
	{% endfor %}
      </ul>
    </div>
  </div>
  <div class="col-sm-9">
    <h1>{{ page.title }}</h1>
    {% block content %}aaaa!{% endblock %}
  </div>
</div>
{% endblock %}
